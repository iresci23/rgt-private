{
    "private": true,
    "scripts": {
        "dev": "npm run development && npm run development-css",
        "dev-js": "npm run development",
        "dev-css": "npm run development-css",
        "development": "cross-env NODE_ENV=development COMP_TYPE=js node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "development-css": "cross-env NODE_ENV=development COMP_TYPE=css node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build": "npm run production && npm run production-css",
        "prod": "npm run production && npm run production-css",
        "production": "cross-env NODE_ENV=production COMP_TYPE=js node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "production-css": "cross-env NODE_ENV=production COMP_TYPE=css node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "axios": "^0.17",
        "bootstrap-sass": "^3.3.7",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^5.0.5",
        "lodash": "^4.17.4",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.26.11",
        "sass-loader": "^8.0.2",
        "vue": "^2.6.12",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "ag-grid-community": "^24.1.0",
        "ag-grid-vue": "^24.1.1",
        "downloadjs": "^1.4.7",
        "keen-ui": "^1.3.1",
        "vue-class-component": "^7.2.6",
        "vue-content-loader": "^0.2.3",
        "vue-property-decorator": "^9.0.2",
        "vuedraggable": "^2.24.1"
    }
}
